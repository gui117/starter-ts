{
  "name": "@gui117/package",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.8.0",
  "description": "_description_",
  "license": "MIT",
  "homepage": "https://github.com/gui117/@gui117/package#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/gui117/@gui117/package"
  },
  "bugs": "https://github.com/gui117/@gui117/package/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.1",
    "@antfu/ni": "^0.22.4",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^22.5.0",
    "bumpp": "^9.5.2",
    "eslint": "^9.9.0",
    "eslint-plugin-format": "^0.1.2",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.9",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "vite": "^5.4.2",
    "vitest": "^2.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
